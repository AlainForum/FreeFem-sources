include "test.idp"

cout << "rint" << endl;
real t;

cout << IND << "real" << endl;

real rpos = 0.5;
real rneg = -0.5;

t = clock();
real respos = rint(rpos);
t = clock() - t;
cout << IND << IND << t << endl;
test(respos == 0.0);

t = clock();
real resneg = rint(rneg);
t = clock() - t;
cout << IND << IND << t << endl;
test(resneg == -0.0);
